{
  "name": "Keyboard Tab Move",
  "version": "0.1",
  "description": "Creates keyboard shortcuts (Alt + Shift + Left/Right) to move currently selected tab to adjacent windows",
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "commands": {
    "move-left": {
      "suggested_key": { "default": "Alt+Shift+Left" },
      "description": "Move tab one window left"
    },
    "move-right": {
      "suggested_key": { "default": "Alt+Shift+Right" },
      "description": "Move tab one window right"
    },
    "move-up": {
      "suggested_key": { "default": "Alt+Shift+Up" },
      "description": "Move tab one window up"
    },
    "move-down": {
      "suggested_key": { "default": "Alt+Shift+Down" },
      "description": "Move tab one window down"
    }
  },
  "permissions": [
      "storage"
  ],
  "manifest_version": 2
}
