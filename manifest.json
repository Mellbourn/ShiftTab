{
  "name": "Shift Tab",
  "version": "1.3",
  "manifest_version": 3,
  "description": "Creates keyboard shortcuts (Ctrl + Shift + Left/Right/Up/Down) to move currently selected tab to adjacent windows",
  "background": {
    "service_worker": "background.js"
  },
  "icons": { "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png" },
  "action": {
    "default_icon": {
             "16": "icon16.png",
             "48": "icon48.png",
            "128": "icon128.png"
    }
  },
  "commands": {
    "move-left": {
      "suggested_key": { "default": "Ctrl+Shift+Left" },
      "description": "Move tab one window left"
    },
    "move-right": {
      "suggested_key": { "default": "Ctrl+Shift+Right" },
      "description": "Move tab one window right"
    },
    "move-up": {
      "suggested_key": { "default": "Ctrl+Shift+Up" },
      "description": "Move tab one window up"
    },
    "move-down": {
      "suggested_key": { "default": "Ctrl+Shift+Down" },
      "description": "Move tab one window down"
    }
  },
  "permissions": []
}
